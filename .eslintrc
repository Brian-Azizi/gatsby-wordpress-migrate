{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error",
    "max-len": ["error", 80]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "IMAGE_COUNT": true,
    "TOTAL_IMAGES": true,
    "IMAGE_ERRORS": true
  }
}
